# Regulatory Intelligence System - Environment Variables
# Copy to .env and fill in your actual values

# Database Configuration (Supabase)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
DATABASE_URL=postgresql://postgres:[password]@db.[project].supabase.co:5432/postgres

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key

# External APIs
FEDERAL_REGISTER_API_KEY=your-federal-register-api-key

# Redis Configuration (for Railway deployment)
REDIS_URL=redis://localhost:6379

# Application Settings
ENV=development
DEBUG=true
LOG_LEVEL=INFO

# Railway Deployment (add when deploying)
PORT=8000
RAILWAY_ENVIRONMENT=production

# Security
SECRET_KEY=your-secret-key-for-jwt-etc
ALLOWED_ORIGINS=http://localhost:3000,https://your-frontend-domain.vercel.app